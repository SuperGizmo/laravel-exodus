{
  "name": "eyf/laravel-exodus",
  "description": "Laravel YAML migrations",
  "keywords": [
    "laravel",
    "migrations",
    "database",
    "yaml",
    "json"
  ],
  "homepage": "https://github.com/eightyfive/laravel-exodus",
  "license": "MIT",
  "authors": [
    {
      "name": "85"
    }
  ],
  "require": {
    "php": ">=7.0.33",
    "illuminate/support": "^5.8|~6.0|~7.0",
    "illuminate/filesystem": "^5.8|~6.0|~7.0",
    "symfony/yaml": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "spatie/phpunit-watcher": "^1.22"
  },
  "autoload": {
    "psr-4": {
      "Eyf\\Exodus\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Eyf\\Exodus\\ExodusServiceProvider"
      ]
    }
  }
}
