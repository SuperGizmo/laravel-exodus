{
    "name": "eyf/laravel-exodus",
    "description": "Laravel YAML migrations",
    "keywords": [
        "laravel",
        "migrations",
        "yaml",
        "database"
    ],
    "homepage": "https://github.com/eightyfive/laravel-exodus",
    "license": "MIT",
    "authors": [
        {
            "name": "85"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.16",
        "illuminate/filesystem": "^9.16",
        "symfony/yaml": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Eyf\\Exodus\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Eyf\\Exodus\\ExodusServiceProvider"
            ]
        }
    }
}
